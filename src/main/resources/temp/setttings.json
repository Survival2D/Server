          "streaming": {
            "enable": true
          },
          "plugins": {
            "size": 1,
            "plugins": [
              {
                "listenEvents": "[USER_LOGIN]",
                "folder": "survival2d",
                "threadPoolSize": 3,
                "configFile": "",
                "name": "survival2d",
                "activeProfiles": null,
                "entryLoader": "com.survival2d.server.ServerStartup$Survival2dAppEntryLoader",
                "id": 1,
                "packageName": null,
                "priority": 1
              }
            ]
          },
          "configFile": "",
          "name": "survival2d",
          "id": 1,
          "eventControllers": {
            "eventControllers": []
          },
          "applications": {
            "size": 1,
            "apps": [
              {
                "maxUsers": 9999,
                "folder": "survival2d",
                "threadPoolSize": 3,
                "configFile": "",
                "name": "survival2d",
                "activeProfiles": null,
                "entryLoader": "com.survival2d.server.ServerStartup$Survival2dAppEntryLoader",
                "id": 1,
                "packageName": null
              }
            ]
          }
        }
      ]
    },
    "eventControllers": {
      "eventControllers": [
        {
          "controller": "com.survival2d.server.network.plugin.StreamingController",
          "eventType": "STREAMING"
        }
      ]
    },
    "streaming": {
      "enable": true
    },
    "threadPoolSize": {
      "extensionRequestHandler": 8,
      "socketUserRemovalHandler": 3,
      "streamHandler": 8,
      "socketDisconnectionHandler": 2,
      "socketDataReceiver": 8,
      "statistics": 1,
      "systemRequestHandler": 8
    },
    "websocket": {
      "codecCreator": "com.tvd12.ezyfox.codec.JacksonCodecCreator",
      "address": "0.0.0.0",
      "sslConfig": {
        "file": "ssl-config.properties",
        "loader": "com.tvd12.ezyfoxserver.ssl.EzySimpleSslConfigLoader",
        "contextFactoryBuilder": "com.tvd12.ezyfoxserver.ssl.EzySimpleSslContextFactoryBuilder"
