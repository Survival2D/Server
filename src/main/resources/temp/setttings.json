      "codecCreator": "com.tvd12.ezyfox.codec.MsgPackCodecCreator",
      "address": "0.0.0.0",
      "maxRequestSize": 1024,
      "port": 3005,
      "sslActive": false,
      "active": true,
      "tcpNoDelay": true,
      "writerThreadPoolSize": 8
