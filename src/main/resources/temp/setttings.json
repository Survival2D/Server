{
  "settings": {
    "nodeName": "survival2d",
    "udp": {
      "channelPoolSize": 16,
      "codecCreator": "com.tvd12.ezyfox.codec.MsgPackCodecCreator",
      "address": "0.0.0.0",
      "maxRequestSize": 1024,
      "port": 2611,
      "active": true,
      "handlerThreadPoolSize": 5
    },
    "debug": true,
    "maxSessions": 999999,
    "sessionManagement": {
      "sessionMaxRequestPerSecond": {
        "action": "DROP_REQUEST",
